  "private": true,
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "web:build": "ionic build --minifyjs --minifycss",
    "build": "ionic-app-scripts build --webpack ./webpack.config.js",
    "lint": "tslint --fix   --project tslint.json ./src/**/*.ts -e ./src/libs/**/*.ts",
    "serve": "ionic-app-scripts serve --webpack ./webpack.config.js",
    "browser": "ionic cordova run browser --livereload --debug --address 0.0.0.0",
    "build:browser": "ionic cordova build browser  --minifyjs --minifycss",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "debug:android": "ionic cordova run android -l -c",
    "build:android": "ionic cordova build android --minifyjs --minifycss --debug ",
    "release:android": "ionic cordova build android --minifyjs --minifycss --release ",
    "debug:ios": "ionic cordova emulate ios -l --debug -c -- --buildFlag='-UseModernBuildSystem=0'  ",
    "build:ios": "ionic cordova build ios --release  --minifyjs --minifycss --buildFlag='-UseModernBuildSystem=0'",
    "rm:android": "ionic cordova platform rm android",
    "add:android": "ionic cordova platform add android",
    "rm:ios": "ionic cordova platform rm ios",
    "test": "karma start ./config/karma.conf.js",
    "test-ci": "karma start ./config/karma.conf.js --single-run",
    "test-coverage": "karma start ./config/karma.conf.js --coverage",
    "e2e": "npm run e2e-update && npm run e2e-test",
    "e2e-test": "protractor ./config/protractor.conf.js",
    "e2e-update": "webdriver-manager update --standalone false --gecko false",
    "debug:electron": "tsc main.ts && electron main.js",
    "build:electron:mac": "tsc main.ts && npx electron-builder build --mac",
    "build:electron:win": "tsc main.ts && npx electron-builder build --win"
  },
